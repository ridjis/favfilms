<div class="jumbotron text-center">
	<h1>{{header}}</h1>
</div>

<div class="wrapper">
	<table class="table table-bordered table-hover table-striped">
		<thead>
			<tr>
				<th>Year</th>
				<th>Number of favorites</th>
			</tr>
		</thead>
		<tbody>
			{{#each results}}
				<tr>
					<td>{{this._id}}</td>
					<td>{{this.value}}</td>
				</tr>
			{{/each}}
		</tbody>
	</table>
</div>